#include <stdio.h>
#include <math.h>

int main() {
    double x_nac, x_end, dx, e;    
    scanf_s("%lf", &x_nac);   
    scanf_s("%lf", &x_end);  
    scanf_s("%lf", &dx);
    scanf_s("%lf", &e);
    printf("\nresultati dlya sin(x) shitaem dlya tochnosti %g:\n", e);
    printf("--------------------------------------------\n");
    printf("    x      shitaem sin(x)  standarniy sin(x)\n");
    printf("--------------------------------------------\n");

    
    for (double x = x_nac; x <= x_end; x += dx) {
        double term = x;  
        double sum = term;
        int n = 0;      
        while (fabs(term) >= e) {
            n++;
            term *= -x * x / ((2 * n) * (2 * n + 1)); 
            sum += term;
        } 
        printf("%6.2lf  %18.6lf  %18.6lf\n", x, sum, sin(x));
    }
    return 0;
}

